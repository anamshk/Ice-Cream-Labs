<!DOCTYPE html>
<html lang="en">

<head>
  <title>Admin: Menu </title>

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
</head>

<body>
  <h1></h1>
  <% for(let item of items) { %>
    <h2><%=item.title%></h2>
    <tr>
      <td><%= item.description %></td>
      <newline><td><%= item.photo_url %></td></newline>
      <newline><td><%= item.price %></td></newline> 
      <newline><td><%= item.inventory %></td></newline>
      <td><form method="GET" action="/admin/admin_edit/<%= item.id %>"></td>
        <button type="submit" class="btn btn-outline-primary">Edit</button>
    </tr>
  <% } %> 
  <form class="form-inline" action="/admin/admin_edit/" method="GET">
    <button type="submit" class="btn btn-outline-primary">Add item</button>

</html>